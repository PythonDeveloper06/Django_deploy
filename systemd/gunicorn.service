[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=pavel
Group=pavel
WorkingDirectory=/home/pavel/DjangoWeb/
ExecStart=/home/pavel/DjangoWeb/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/run/gunicorn.sock DjangoWeb.wsgi:application


[Install]
WantedBy=multi-user.target